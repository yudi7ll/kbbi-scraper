// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DB_URL")
}

model Word {
  id   String @id @default(auto()) @map("_id") @db.ObjectId
  kata String @unique
  data Lema[]
}

type Lema {
  lema String
  arti Arti[]
}

type Arti {
  kelas_kata String
  deskripsi  String
}
